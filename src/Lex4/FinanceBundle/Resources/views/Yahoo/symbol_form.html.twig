{% extends 'Lex4FinanceBundle:Yahoo:index.html.twig' %}
{% block content_layout %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">{{ title }}</h3>
        </div>
        <div class="panel-body">
            {{ form_start(form) }}
            <div class="form_row">
                {{ form_errors(form) }}
                {{ form_widget(form) }}
            </div>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}